<section xmlns="http://www.music-encoding.org/ns/mei" xml:id="section1" type="sectiontype">
    <pb/>
    <annot type="verovio-options">
      {
        "svgHtml5": false
      }
    </annot>
    <annot label="write MEI element names as classes" type="test">
        assert.ok(svg.querySelector(".note"), "class on note");
        assert.ok(svg.querySelector(".slur"), "class on slur");
    </annot>
    <annot label="write spanning classes for elements crossing system breaks" type="test">
        assert.ok(svg.querySelector(".spanning.slur.id-slur1"));
    </annot>
    <annot label="convert @type to class" type="test">
        assert.ok(svg.querySelector(".notetype"), "type on note");
        assert.ok(svg.querySelector(".dirtype"), "type on dir");
    </annot>
    <annot label="convert @xml:id to @id" type="test">
        assert.ok(svg.querySelector("#note1"));
    </annot>
    <annot label="add autogenerated classes" type="test">
        assert.ok(svg.querySelector(".mNum.autogenerated"));
    </annot>
    <annot label="add boundaryStart/boundaryEnd classes" type="test">
        assert.ok(svg.querySelector("#section1.boundaryStart"), "section boundary start");
        assert.ok(svg.querySelector(".section1.boundaryEnd"), "section boundary end");
    </annot>
    <annot label="write ledgerline classes" type="test">
        assert.ok(svg.querySelector(".ledgerLines.below.cue"));
    </annot>
    <annot label="write barLineAttr classes" type="test">
        assert.ok(svg.querySelector(".barLineAttr"));
    </annot>
    <measure n="1">
        <staff>
            <layer>
                <note dur="1" pname="g" oct="4" type="notetype" xml:id="note1"/>
            </layer>
        </staff>
        <slur startid="#note1" endid="#note2" type="slurtype" xml:id="slur1"/>
        <dir tstamp="1" type="dirtype">foo</dir>
    </measure>
    <sb/>
    <measure n="2" right="rptend">
        <staff>
            <layer>
                <note dur="1" pname="c" oct="4" xml:id="note2" cue="true"/>
            </layer>
        </staff>
    </measure>
    <sb/>
</section>
